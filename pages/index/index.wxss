.headtop{
  width: 100%;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  text-align: center;
  vertical-align: middle;
  font-size: 30rpx;
  color:#ffffff;
  background-color:#000000;
}
.headtop_img {
  width: 20%;
}

.tabbar
{
display: flex;
height: 60rpx;
padding-top:20rpx; 
text-align: center;
}

.headtop_location{
  text-align: center;
  width: 60%;
}


.memberinfo{
  width: 100%;
  height: 450rpx;
  background-color: #191919;
}

.memberavatar{
  width: 100%;
  height: 160rpx;
  text-align: center;
  margin-top:-80rpx;
}

.membername{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color:#ffffff;
}

.memberorder{
  display: flex;
  width: 100%;
  text-align: center;
  padding-top: 20rpx;
}
.member{
  width: 33.3%;
  text-align: center;
}


.membertitle{
  color:#666666;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.membervalue{
  color:#ffffff;
  font-size: 40rpx;
}

.member label{
  color:#ffffff;
  font-size: 20rpx;
}


.orderinfo{
  background-color: #f3f3f3;
  width: 100%;
}

.infolist{
  background-color: #ffffff;
  padding-top:20rpx;
   padding-left: 6%;
   padding-right: 6%;
   padding-bottom: 20rpx;

   border-top: 1rpx #e8e8e8 solid;
  
}

.infotitle{
  display: flex;
  line-height: 50rpx;
  height: 60rpx;
}

.infolocation{
  color:black;
  font-size:35rpx;
  display: flex;
}

.loctiondest{
  padding-left:15rpx;
  height: 60rpx;
  line-height: 60rpx;
font-size:35rpx;
display:block;
}

.labelshishi{
  border:1rpx #ED1C24 solid;
  font-size:20rpx;
  padding: 5rpx 10rpx;
  color:#ED1C24;
  box-shadow: 1rpx 1rpx 5px #888888;
}

.labelyuyue{
  border:1rpx blue solid;
  font-size:20rpx;
  padding: 5rpx 10rpx;
  color:blue;
  box-shadow: 1rpx 1rpx 5px #888888;
}


.tab_bottom{
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  text-align: center;

}

.buttonstart{
  width:200rpx;
  height:200rpx;
  border:1rpx #ED1C24 solid;
  border-radius:50%;
  margin:0 auto;
  text-align:center;
  line-height:200rpx;
  color:white;
  font-size: 35rpx;
  font-weight: bold;
  background-color:#ED1C24;
}